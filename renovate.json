{
  "baseBranch": "develop",
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["maven", "npm", "circleci", "dockerfile", "gradle", "gradle-wrapper"],
  "commitMessageExtra" : "from v{{currentVersion}} to {{#if isMajor}}v{{{newMajor}}}{{else}}{{#if isSingleVersion}}v{{{toVersion}}}{{else}}{{{newValue}}}{{/if}}{{/if}}",
  "major": {
    "automerge": false
  },
  "minor": {
    "automerge": true
  },
  "patch": {
    "automerge": true
  },
  "pin": {
    "automerge": true
  },
  "digest": {
    "automerge": true
  },
  "hostRules": [
    {
      "baseUrl": "https://entur2.jfrog.io/entur2/",
      "encrypted": {
        "password": "",
        "username": ""
      }
    }
  ],
  "packageRules": [
    {
      "packagePatterns": ["^no.entur"],
      "updateTypes": ["minor","patch", "pin", "digest"],
      "automerge": true,
      "managers": [
        "maven",
        "gradle",
        "gradle-wrapper"
      ],
      "registryUrls": [
        "https://entur2.jfrog.io/entur2/entur-team-kontobasert-billettering-release"
      ]
    },
    {
      "packagePatterns": ["^@entur-private"],
      "updateTypes": ["minor","patch", "pin", "digest"],
      "automerge": true,
      "managers": ["npm"]
    }
  ]
}
